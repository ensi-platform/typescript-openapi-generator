{
    "name": "@ensi-platform/typescript-openapi-generator",
    "version": "2.0.0-beta.3",
    "description": "Generate TS types and react-query hooks from OpenAPIV3 specification\nFor more information use: https://docs.ensi.tech/",
    "type": "module",
    "private": false,
    "scripts": {
        "build": "rimraf dist && vite build",
        "init": "pnpm run build && node ./dist/bin/init.js",
        "generate": "pnpm run build && node ./dist/bin/generate.js",
        "prepublishOnly": "npm run build",
        "lint": "eslint . --ext .ts --fix",
        "format": "prettier ./src --write --ignore-path .prettierignore",
        "publish": "npm publish --access public --tag beta"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ensi-platform/typescript-openapi-generator"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "bugs": "https://github.com/ensi-platform/typescript-openapi-generator/issues",
    "keywords": [
        "ensi",
        "front-end",
        "orval",
        "typescript",
        "openapi"
    ],
    "homepage": "https://github.com/ensi-platform/typescript-openapi-generator",
    "files": [
        "/dist/*",
        "/npm-shrinkwrap.json"
    ],
    "license": "MIT",
    "dependencies": {
        "@stoplight/yaml": "^4.3.0",
        "lodash.clonedeep": "^4.5.0",
        "openapi-typescript": "^6.7.6",
        "orval": "^7.7.0",
        "prettier": "^2.8.8",
        "rimraf": "^6.0.1",
        "yaml": "^2.5.1"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^28.0.3",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/lodash.clonedeep": "^4.5.9",
        "@types/node": "^18.19.50",
        "@types/rimraf": "^4.0.5",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "eslint": "^8.57.1",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-unicorn": "^47.0.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "ts-import": "2.0.40",
        "tslib": "^2.7.0",
        "typescript": "^5.6.2",
        "vite": "^6.2.6",
        "vite-plugin-dts": "^4.5.3",
        "vite-plugin-static-copy": "^2.3.1"
    },
    "peerDependencies": {
        "orval": "^7.7.0"
    }
}
